generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Invoice {
    id             String                 @id @default(uuid())
    imageUrl       String
    extractedText  String
    structuredData Json
    createdAt      DateTime               @default(now())
    embeddings     Unsupported("vector")? // Adjust dimension based on embedding model
}
